.reveal table {
    margin: auto;
    border-collapse: collapse;
    border-spacing: 0;
}
.sl-block.submit-btn-output p {
  display: none;
}
.reveal .sl-block[data-block-type="image"] .sl-block-content img {
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
    border: 0;
    vertical-align: top;
}
video::-internal-media-controls-download-button {
    display:none;
}
video::-webkit-media-controls-enclosure {
    overflow:hidden;
}
video::-webkit-media-controls-panel {
    width: calc(100% + 30px); /* Adjust as needed */
}
.sl-block span.ref:after {
   content: ',';
   /*position: absolute;*/
}
.sl-block span.ref:last-child:after {
   display: none;
}
.sl-block-content.intialP > p {
    font-size: 10px;
}
sub{
