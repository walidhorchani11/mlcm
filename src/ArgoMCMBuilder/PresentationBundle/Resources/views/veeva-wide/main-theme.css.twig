#stage>.content>.reveal>.slides>section {
    display: block !important;
    opacity: 1;
}

#menu {
    position: absolute;
    width: 1024px;
    height: 110px;
    bottom: 0;
    z-index: 950;
    overflow: hidden;
}

#menu .maxMenu, #menu .subMenu {
    position: absolute;
    bottom: 0;
    height: 65px;
    z-index: 1;
    white-space: nowrap;
    -webkit-overflow-scrolling: touch;
}

#menu .maxMenu {
    height: 65px;
    left: 100px;
    margin-top: 0px;
}
#menu .maxMenu a.current{
    color: rgb(62, 135, 135);
    {% if selected %}
        background-image: url('../shared/{{ shared }}/theme/theme1/images/selected.png');
        background-repeat: no-repeat;
        background-position: center bottom;
        background-size: 80px;
    {% endif %}
}
#menu .subMenu a.current {
    color: rgb(62, 135, 135);
    {% if selected %}
        background-image: url('../shared/{{ shared|raw }}/theme/theme1/images/selected.png');
        background-repeat: no-repeat;
        background-position: center 11px;
        background-size: 80px;
    {% endif %}
}

#menu a.home {
    width: 100px;
    height: 46px;
{% if logoHome is not empty %}
    background: url('../shared/{{ logoHome|raw }}');
    background-position: 60px center !important;

 {%  else %}
    background: url('../shared/{{ shared|raw}}/theme/theme1/images/home.png');
    background-position: 60px 8px !important;
 {% endif %}
    border-bottom-right-radius: 0;
    border: none;
    position: absolute;
    bottom: 0;
    z-index: 2;
     background-color: #4A5667;
    top: 55px;
    background-size: 30px auto;
    background-repeat: no-repeat;

}

#menu .maxMenu a, #menu .subMenu a {
    font-family: "Montserrat";
    color: white;
    width: auto;
    text-align: center;
    vertical-align: middle;
    cursor: pointer;
    line-height: 20px;
    white-space: pre-wrap;
    word-break: break-word;
    display: inline-block;
    font-size: 14px;
}

#menu .subMenu a {
    padding: 0 10px !important;
}
#menu .maxMenu a {
    height: 60px;
    margin-top: 0;
    display: inline-block;
    line-height: 60px;
    padding: 0 15px !important;
}
#menu .maxMenu a:first-child {
    padding-left: 0 !important;
}

#menu .maxMenu a:nth-child(2) {
    padding-left: 0;
}

#menu .maxMenu a:last-child {
    border: none;
    padding-right: 0px;
}

/* SubMenu */

#menu .subPicto {
    width: 175.5px;
    height: 28.5px;
    position: absolute;
    top: 0;
    left: 0;
    background: url('../shared/{{ shared|raw }}/theme/theme1/images/sub.png') center center / 175.5px 28.5px no-repeat;
    z-index: 2;
}

#menu .subMenu.current {
    display: inline-flex !important;
}

#menu .subMenu {
    display: none;
    top: 18px;
    height: 25px;
    z-index: 0;
    background: #4a5667;
    white-space: normal;
    overflow: hidden;
    max-width: 450px;
    border-radius: 0 5px 5px 0;
    -moz-border-radius: 0 5px 5px 0;
    -webkit-border-radius: 0 5px 5px 0;
}


#menu .subMenu a {
    font-size: 15px;
    font-family: "Montserrat";
    line-height: 25px;
}

/* minMenu */
#menu .minMenu {
    position: absolute;
    width: 200px;
    height: 65px;
    bottom: 0;
    right: 0;
    z-index: 99;
    background: #4a5667;
}
#menu .minMenu div, #menu .minMenu a {
    position: absolute;
    cursor: pointer;
}

